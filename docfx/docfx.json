{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "Xamarin.Preferences/Xamarin.Preferences.csproj",
            "Xamarin.ProcessControl/Xamarin.ProcessControl.csproj",
            "Xamarin.Helpers/Xamarin.Helpers.csproj",
            "Xamarin.NativeHelpers/Xamarin.NativeHelpers.csproj",
            "Xamarin.Downloader/Xamarin.Downloader.csproj",
            "Xamarin.MSBuild.Sdk/Xamarin.MSBuild.Sdk.csproj",
            "Xamarin.XunitHelpers/Xamarin.XunitHelpers.csproj",
            "Xamarin.Security.Keychain/Xamarin.Security.Keychain.csproj",
            "Xamarin.BuildConsolidator/Xamarin.BuildConsolidator.csproj"
          ],
          "exclude": []
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}